{
  "expo": {
    "name": "estate",
    "slug": "estate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/test.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/test.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/test.png",
        "backgroundColor": "#000000"
      },
      "package": "com.goodie007.estate"
    },
    "web": {
      "favicon": "./assets/images/test.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
